{
    "name": "kregel/laravel-flight",
    "description": "A Laravel Package that uses Laravel Socialite to access Laravel tools like Horizon, and Telescope",
    "keywords": [
        "kregel",
        "laravel-flight"
    ],
    "homepage": "https://github.com/kregel/laravel-flight",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Austin Kregel",
            "email": "5355937+austinkregel@users.noreply.github.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "laravel/socialite": "^5.6",
        "socialiteproviders/laravelpassport": "^4.0",
        "socialiteproviders/manager": "^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Kregel\\Flight\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kregel\\Flight\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kregel\\LaravelFlight\\LaravelFlightServiceProvider"
            ],
            "aliases": {
                "LaravelFlight": "Kregel\\LaravelFlight\\LaravelFlightFacade"
            }
        }
    }
}
